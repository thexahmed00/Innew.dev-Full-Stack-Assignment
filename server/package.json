{
  "name": "server",
  "version": "1.0.0",
  "description": "Express TypeScript API Server",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "dev:build": "tsc && node dist/server.js",
    "clean": "rm -rf dist",
    "migrate": "node scripts/migrate.js",
    "migrate:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "stripe:setup": "npm run build && node scripts/setup-stripe-products.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.872.0",
    "@prisma/client": "^6.14.0",
    "@supabase/supabase-js": "^2.54.0",
    "@types/multer": "^2.0.0",
    "@types/sharp": "^0.31.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "prisma": "^6.14.0",
    "resend": "^6.0.1",
    "sharp": "^0.34.3",
    "stripe": "^14.25.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
